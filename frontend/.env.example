# Database (Required for Prisma)
DATABASE_URL="postgresql://user:password@host:5432/urgentcare?schema=public"

# NextAuth (Required)
NEXTAUTH_SECRET="generate-with-openssl-rand-base64-32"
NEXTAUTH_URL="http://localhost:3000"

# Backend API (Optional - defaults to localhost)
NEXT_PUBLIC_BACKEND_URL="http://localhost:5099"

# AWS S3 (Optional - for document storage in Phase 5)
AWS_REGION="us-east-1"
AWS_S3_BUCKET="your-bucket-name"
AWS_ACCESS_KEY_ID="your-access-key"
AWS_SECRET_ACCESS_KEY="your-secret-key"

# AI Providers (Required for Phase 3+)
# OpenAI API Key - For Whisper transcription (Phase 3)
OPENAI_API_KEY="sk-..."

# Anthropic API Key - For Claude SOAP note generation (Phase 4)
ANTHROPIC_API_KEY="sk-ant-..."
